{
  "name": "prusov-code/json-yaml-comparator",
  "license": "MIT",
  "version": "1",
  "description": "A simple PHP library and command-line utility for comparing JSON and YAML files with multiple output formats",
  "keywords": ["json", "yaml", "comparator", "diff"],
  "autoload": {
    "psr-4": {
      "PrusovCode\\JsonYamlComparator\\": "src/",
      "PrusovCode\\JsonYamlComparator\\Tests\\": "tests/"
    },
    "files": [
      "src/Comparator.php"
    ]
  },
  "bin": [
    "bin/compare"
  ],
  "authors": [
    {
      "name": "prusov-code",
      "email": "prusov-code@yandex.ru"
    }
  ],
  "require": {
    "docopt/docopt": "^1.0",
    "symfony/var-dumper": "^7.3",
    "funct/funct": "^1.6",
    "symfony/yaml": "^7.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^4.0",
    "phpunit/phpunit": "^12.4",
    "phpstan/phpstan": "^2.1"
  },
  "minimum-stability": "dev"
}
